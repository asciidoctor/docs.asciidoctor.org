{
  "name": "asciidoctor-docs-netlify",
  "description": "Asciidoctor Docs (Production and Deploy Previews)",
  "private": true,
  "scripts": {
    "clean-build": "npm run clean-install;npm run build",
    "clean-install": "npm i --cache=.cache/npm --no-optional",
    "check": "antora --fetch --generator=@antora/xref-validator ../antora-playbook.yml",
    "quick-build": "antora ../antora-playbook.yml --stacktrace",
    "build": "npm run check;npm run quick-build"
  },
  "devDependencies": {
    "@antora/cli": "3.0.0-alpha.1",
    "@antora/site-generator-default": "3.0.0-alpha.1",
    "@antora/xref-validator": "git+https://gitlab.com/antora/xref-validator#v1.0.0-alpha.13"
  }
}
